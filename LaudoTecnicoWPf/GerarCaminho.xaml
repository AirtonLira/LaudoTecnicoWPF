<Window x:Class="LaudoTecnicoWPF.GerarCaminho"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaudoTecnicoWPF"
        mc:Ignorable="d"
        Title="GerarCaminho" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Margin="10" Text="Numero da gmud" Name="numerogmud"></TextBox>
            <StackPanel Orientation="Horizontal">
                <RadioButton Margin="10" Name="pch" Checked="pch_Checked">PCH</RadioButton>
                <RadioButton Margin="10" Name="especifico"  Checked="especifico_Checked">Especifico</RadioButton>
            </StackPanel>
            <TextBox Name="emissor" Margin="10">Qual emissor?</TextBox>
            <CheckBox Name="reversao" Margin="10" >Possui Reversão?</CheckBox>
            <TextBox Name="caminho" Margin="10">\\10.1.1.14\ScriptsDesenv</TextBox>
            <Button Name="gerar" Background="BlueViolet" Margin="10" Click="gerar_Click">Gerar caminho</Button>

        </StackPanel>
    </Grid>
</Window>
